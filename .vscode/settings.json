// Placez vos paramètres dans ce fichier pour remplacer les paramètres par défaut et les paramètres utilisateur.
{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "search.exclude": {
        "**/.git": true,
        "**/*.log": true,
        "node_modules/": true,
        "packages/": true,
        "tmp/": true
    },
    "cSpell.enableFiletypes": [
        "!asciidoc",
        "!c",
        "!cpp",
        "!csharp",
        "!css",
        "!git-commit",
        "!go",
        "!graphql",
        "!handlebars",
        "!haskell",
        "!html",
        "!jade",
        "!java",
        "!javascript",
        "!javascriptreact",
        "!json",
        "!jsonc",
        "!latex",
        "!less",
        "!php",
        "!plaintext",
        "!pug",
        "!python",
        "!restructuredtext",
        "!rust",
        "!scala",
        "!scss",
        "!text",
        "!typescript",
        "!typescriptreact",
        "!yaml",
        "!yml"
    ],
    "cSpell.language": "fr-FR"
}